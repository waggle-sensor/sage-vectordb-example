{
  "dashboard": {
    "id": null,
    "title": "Weavloader Metrics",
    "tags": ["weavloader", "celery", "redis"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Task Processing Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(weavloader_tasks_processed_total[5m])",
            "legendFormat": "Tasks/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Task Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(weavloader_tasks_processed_total{status=\"success\"}[5m]) / rate(weavloader_tasks_processed_total[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Queue Sizes",
        "type": "graph",
        "targets": [
          {
            "expr": "weavloader_queue_size",
            "legendFormat": "{{queue_name}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Dead Letter Queue Size",
        "type": "graph",
        "targets": [
          {
            "expr": "weavloader_dlq_size",
            "legendFormat": "DLQ Size"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 5,
        "title": "Task Processing Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(weavloader_task_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(weavloader_task_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 6,
        "title": "Active Workers",
        "type": "stat",
        "targets": [
          {
            "expr": "weavloader_active_workers",
            "legendFormat": "Workers"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 24}
      },
      {
        "id": 7,
        "title": "System Health",
        "type": "stat",
        "targets": [
          {
            "expr": "weavloader_system_health",
            "legendFormat": "System Health"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 8, "y": 24}
      },
      {
        "id": 8,
        "title": "SAGE Stream Health",
        "type": "stat",
        "targets": [
          {
            "expr": "weavloader_sage_stream_health",
            "legendFormat": "SAGE Stream"
          }
        ],
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 24}
      },
      {
        "id": 9,
        "title": "Error Rate by Component",
        "type": "graph",
        "targets": [
          {
            "expr": "weavloader_error_rate",
            "legendFormat": "{{component}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 32}
      },
      {
        "id": 10,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "weavloader_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "{{component}} MB"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 40}
      },
      {
        "id": 11,
        "title": "Model Inference Duration",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(weavloader_model_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "{{model_name}} {{operation}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 40}
      },
      {
        "id": 12,
        "title": "SAGE Images Received",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(weavloader_sage_images_received_total[5m])",
            "legendFormat": "{{node_id}} {{camera}}"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 48}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "30s"
  }
}
